<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.wxportal.dao.bean">

    <class name="UserBean" table="user">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="nickName" type="java.lang.String" column="nickname" length="20"/>
        <property name="passWord" type="java.lang.String" column="password" length="32"/>
        <property name="level" type="java.lang.String" column="level" length="20"/>
        <property name="email" type="java.lang.String" column="email" length="20"/>
        <property name="telphone" type="java.lang.String" column="telephone" length="20"/>
        <property name="addr" type="java.lang.String" column="addr" length="20"/>
        <property name="beginTime" type="java.lang.String" column="begintime" length="20"/>
        <property name="endTime" type="java.lang.String" column="endtime" length="20"/>
        <property name="name" type="java.lang.String" column="name" length="20"/>
        <property name="creditCard" type="java.lang.String" column="creditcard" length="20"/>
    </class>

</hibernate-mapping>
